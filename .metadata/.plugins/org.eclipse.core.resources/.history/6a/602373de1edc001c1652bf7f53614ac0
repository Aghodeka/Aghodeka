package Exercise1;
import java .util.Scanner;


public class AgeException {
	public static void AgeException (String[]args){
		
      Scanner sc = new Scanner (System.in);
      System.out.println("enter your age");
      int age = sc.nextInt();
      
      try
      {
    	  if (age < 15)
    	  {
    		  throw new Exception();
    	  }
      }
    	  catch(Exception ex)
      {
    	  System.out.println(ex);
      }
     

	}
}
      

     
    

    
    
    


	
    
    
	


