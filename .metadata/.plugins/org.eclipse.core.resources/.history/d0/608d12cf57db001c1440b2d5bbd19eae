package Exercise3;

public class ReversNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    
    	int[] num= {1234};
    	int ans = 0;
    	while(num>0) {
    		int num1 = num1 % 10;
    		int rem;
			ans = ans *10+rem;
    		num=num / 10;
    		
    	
    }
     System.out.println(ans);
	}
}


